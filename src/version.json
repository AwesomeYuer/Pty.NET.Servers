{
    "version": "0.4",
    "publicReleaseRefSpec": [
      "^refs/heads/main$"
    ],
    "cloudBuild": {
      "setVersionVariables": true,
      "buildNumber": {
        "enabled": false,
        "includeCommitId": {
          "when": "nonPublicReleaseOnly",
          "where": "buildMetadata"
        }
      }
    }
  }